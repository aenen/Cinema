
@{
    Layout = null;
}

<!DOCTYPE html>

<html style="height:100%">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>TRAEKTORIA - Мережа кінотеатрів України</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body class="home-body">
    <nav class="home-navbar">
        <a href="/" class="nav-logo-center"></a>
        <div class="nav-home-item float-left"></div>
        <div class="nav-home-item float-right"></div>
    </nav>
    <div class="home-container">
        <div class="home-logo">
            <img class="home-logo-img" src="~/Content/WebsiteImage/Logo/logo.svg" height="125" />
        </div>
        <div class="home-main-menu">
            <a href="#" class="home-btn">Обрати кінотеатр</a>
            <a href="#" class="home-btn">Обрати фільм</a>
        </div>
        <div class="home-additional-menu">
            <div class="nav-home-item"></div>
            <div class="nav-home-item"></div>
            <div class="nav-home-item"></div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>
